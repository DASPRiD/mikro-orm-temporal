{
  "name": "mikro-orm-temporal",
  "version": "1.0.0",
  "description": "Temporal types for Mikro ORM",
  "type": "module",
  "author": "Ben Scholzen 'DASPRiD'",
  "license": "BSD-3-Clause",
  "keywords": [
    "temporal",
    "mikro-orm",
    "Interval",
    "duration",
    "PlainDate",
    "PlainDateTime",
    "PlainTime",
    "ZonedDateTime",
    "typescript",
    "mysql",
    "mariadb",
    "postgres",
    "mssql",
    "sqlite"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dasprid/mikro-orm-temporal.git"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "c8 tsx --test",
    "test:ci": "c8 --reporter=lcov tsx --test",
    "format": "biome format . --write",
    "check": "biome check . --write"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@mikro-orm/better-sqlite": "^6.4.16",
    "@mikro-orm/core": "^6.4.16",
    "@mikro-orm/mariadb": "^6.4.16",
    "@mikro-orm/mssql": "^6.4.16",
    "@mikro-orm/mysql": "^6.4.16",
    "@mikro-orm/postgresql": "^6.4.16",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.0.3",
    "c8": "^10.1.3",
    "lefthook": "^1.11.14",
    "temporal-polyfill": "^0.3.0",
    "temporal-spec": "^0.3.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@mikro-orm/core": "^6.0.5"
  },
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild",
      "lefthook"
    ]
  }
}
