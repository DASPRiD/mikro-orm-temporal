{
  "name": "mikro-orm-temporal",
  "version": "1.2.1",
  "description": "Temporal types for Mikro ORM",
  "type": "module",
  "author": "Ben Scholzen 'DASPRiD'",
  "license": "BSD-3-Clause",
  "keywords": [
    "temporal",
    "mikro-orm",
    "Interval",
    "duration",
    "PlainDate",
    "PlainDateTime",
    "PlainTime",
    "ZonedDateTime",
    "typescript",
    "mysql",
    "mariadb",
    "postgres",
    "mssql",
    "sqlite"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DASPRiD/mikro-orm-temporal.git"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "c8 tsx --test",
    "test:ci": "c8 --reporter=lcov tsx --test",
    "format": "biome format . --write",
    "check": "biome check . --write"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.7",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@mikro-orm/better-sqlite": "^6.5.9",
    "@mikro-orm/core": "^6.5.9",
    "@mikro-orm/mariadb": "^6.5.9",
    "@mikro-orm/mssql": "^6.5.9",
    "@mikro-orm/mysql": "^6.5.9",
    "@mikro-orm/postgresql": "^6.5.9",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.9.1",
    "c8": "^10.1.3",
    "lefthook": "^2.0.0",
    "temporal-polyfill": "^0.3.0",
    "temporal-spec": "^0.3.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@mikro-orm/core": "^6.0.5"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild",
      "lefthook"
    ]
  }
}
